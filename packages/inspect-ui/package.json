{
  "name": "@effection/inspect-ui",
  "version": "2.1.4",
  "description": "Web interface for inspecting effection applications",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "dist-esm/index.js",
  "module": "dist-esm/index.js",
  "sideEffects": true,
  "homepage": "https://frontside.com/effection",
  "repository": {
    "type": "git",
    "url": "https://github.com/thefrontside/effection.git",
    "directory": "packages/inspect-ui"
  },
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist-*/**/*",
    "app/**/*"
  ],
  "scripts": {
    "prepack": "parcel build --target app && yarn tsc --build tsconfig.esm.json && copyfiles -f app/*.{css,svg} dist-esm",
    "start": "parcel serve --target app",
    "watch": "parcel watch --target app",
    "lint": "eslint '{app,src,tests}/**/*.{ts,tsx}'",
    "test": "mocha -r ts-node/register --timeout 5000 test/**/*.test.{ts,tsx}",
    "docs": "echo noop",
    "mocha": "mocha -r ts-node/register --timeout 5000"
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.29.0",
    "@effection/inspect-utils": "2.1.4",
    "@effection/react": "2.1.3",
    "@effection/websocket-client": "2.0.4",
    "@frontside/tsconfig": "^1.2.0",
    "@types/jsdom-global": "^3.0.2",
    "@types/node": "^13.13.5",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.1",
    "copyfiles": "^2.4.1",
    "effection": "2.0.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "events": "^3.3.0",
    "expect": "^25.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.3.1",
    "parcel": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  },
  "targets": {
    "app": {
      "source": "app/index.html",
      "distDir": "dist-app",
      "optimize": false
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
